<script type="text/javascript">
function inputFocus(obj){
	obj.style.border = "1px solid #75ADF2";
	obj.style.background = "#FFFBE5";
}

function inputBlur(obj){
	obj.style.border = "1px solid #b3d2f8";
	obj.style.background = "#ffffff";
}
</script>

